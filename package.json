    "gravatar": "^1.8.1",
    "i": "^0.3.6",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.25",
    "nodemailer": "^6.4.10",
    "redux-devtools": "^3.5.0",
    "redux-devtools-extension": "^2.13.8",
    "request": "^2.88.2",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.4"
